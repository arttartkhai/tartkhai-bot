{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es6",
    "lib": ["esnext"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "paths": {
      "types/*": ["types/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
