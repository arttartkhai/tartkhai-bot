{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es5",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "moduleResolution": "node",
    "paths": {
      "config/*": ["config/*"],
      "constants/*": ["constants/*"],
      "handle/*": ["handle/*"],
      "services/*": ["services/*"],
      "templates/*": ["templates/*"],
      "types/*": ["types/*"],
      "utils/*": ["utils/*"],
      "webhook/*": ["webhook/*"],
    }
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
