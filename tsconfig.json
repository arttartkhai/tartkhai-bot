{
  "compilerOptions": {
    "baseUrl": "./src",
    // "target": "es6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "moduleResolution": "node",
    "paths": {
      "types/*": ["types/*"],
    }
  },
  "exclude": ["node_modules"],
  // "include": ["src"]
  "include": ["src/**/*.ts"]
}
